{
    "input_path": "../dataset",
    "output_path": "run/results/",
    "training": {
        "use_logger": true,
        "logger_folder_path": "logs/",
        "resize": 32,
        "preprocess_transformations": ["INVERSION"],
        "postprocess_transformations": ["BINARISE"],
        "max_epoch": 50,
        "batch_size": 1,
        "verbose": false,
        "topology": [1024, 64, 64, 32, 32],
        "activation": {
            "identity": {
                "enabled": false

            },
            "sigmoid": {
                "enabled": false

            },
            "relu": {
                "enabled": false

            },
            "leakyRelu": {
                "enabled": false

            },
            "square": {
                "enabled": false

            }
        },

        "optimization": {
            "sgd": {
                "enabled": false,
                "lr": 0.1
            },
            "decay": {
                "enabled": false,
                "lr0": 0.2,
                "dr": 0.1,
            },
            "momentum": {
                "enabled": false,
                "lr": 0.1,
                "mom": 0.9
            },
            "decaymomentum": {
                "enabled": false,
                "lr0": 0.2,
                "dr": 0.1,
                "mom": 0.9,
            },
            "rprop": {
                "enabled": false,
                "eta_p": 1.2,
                "eta_m": 0.5,
                "lr_max": 50.0,
                "lr_min": 1e-6
            }
        },
        "optimizer": {
            "batch": {
                "enabled": false
            },
            "minibatch": {
                "enabled": false,
                "size": 8
            }
        },
        "model": {
            "random": {
                "enabled": false

            },
            "batch": {
                "enabled": false

            }
        }
    }
}